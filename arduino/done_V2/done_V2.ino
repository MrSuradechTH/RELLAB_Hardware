          Serial.println(" " + response + " " + machine_name);
          if (response != NULL) {
//              Serial.println("cehck alarm is " + String(response.indexOf(alarm_string)));
            if (response.indexOf(alarm_string) != -1) {
              alarm_check = true;
            }else {
              alarm_check = false;
            }
          }
        } else {
//            Serial.print("Error on sending POST: ");
          first_post = true;
//            Serial.println(status_code);
        }

        first_post = false;
        millis_old = millis_now;
        digitalWrite(LED, !digitalRead(LED));
      } catch(...) {
//          ESP.restart();
        first_post = true;
      }
    }
    
    vTaskDelay(pdMS_TO_TICKS(100));
  }

  vTaskDelete(NULL);
}

void core_1(void *not_use) {
  while(true) {
    //  String dip_state = String(digitalRead(dip[0])) + String(digitalRead(dip[1])) + String(digitalRead(dip[2])) + String(digitalRead(dip[3]));

    if (alarm_check) {
      rgb_color("red");
      digitalWrite(piezo, HIGH);
    }else {
      rgb_color("green");
      digitalWrite(piezo, LOW);
    }
  
    CAL_ADJUST = ((analogRead(vr) / 4095.00) * (abs(CAL_ADJUST_MIN) + abs(CAL_ADJUST_MAX))) - CAL_ADJUST_MAX;
  
    operate_time = millis();
    if(operate_time > over_time) {
      ESP.restart();
    }
    
    if (!Serial)  {
      Serial.println("Connecting to Serial...");
      while (!Serial)  {
        delay(10);
        Serial.print(".");
        reconnect_count++;
        if (reconnect_count >= 5000) {
          ESP.restart();
        }
      }
  
      reconnect_count = 0;
      
      Serial.println("Connected to Serial");
  
      first_average = true;
      first_post = true;
    }
    
    C = Max31855.readCelsius();
  //  Serial.println(String(C) + " : " + String(CAL_ADJUST));
    C = CAL(C);
  
    if (first_average == true) {
      C = preferences.getDouble("C", C);
      for (uint8_t i = 0; i < C_Stack_Size;i++) {
        C_Stack[i] = C;
      }
    
      first_average = false;
    }
    
    for (uint8_t i = 0; i < C_Stack_Size - 1;i++) {
      C_Stack[i] = C_Stack[i + 1];
    }
    C_Stack[C_Stack_Size - 1] = C;
  
    for (uint8_t i = 0; i < C_Stack_Size;i++) {
      C_Average += C_Stack[i];
    }
  
    C = C_Average / C_Stack_Size;
    C_Average = 0;
    
    display.clearDisplay();
    display.setCursor(0,0);
    
    if (WiFi.status() != WL_CONNECTED) {
      display.println("Wifi : Fail");
      display.setCursor(0,8);
      display.println("IP : XXX.XXX.XXX.XXX");
      display.setCursor(0,16);
      display.println("RSSI : XXX dBm");
      WiFi.begin(ssid, password);
  
      Serial.println("Connecting to WiFi...");
      while (WiFi.status() != WL_CONNECTED) {
        delay(10);
        Serial.print(".");
        reconnect_count++;
        if (reconnect_count >= 5000) {
          ESP.restart();
        }
      }
      
      reconnect_count = 0;
      
      Serial.println("Connected to WiFi");
  
      http.begin(url);
      http.setTimeout(http_timeout);
      http.addHeader("Content-Type", "application/x-www-form-urlencoded");
  
      first_average = true;
      first_post = true;
  
      ArduinoOTA.setHostname(host_name);
  
      ArduinoOTA
        .onStart([]() {
          String type;
          if (ArduinoOTA.getCommand() == U_FLASH)
            type = "sketch";
          else
            type = "filesystem";
          Serial.println("Start updating " + type);
        })
        .onEnd([]() {
          Serial.println("\nEnd");
        })
        .onProgress([](unsigned int progress, unsigned int total) {
          Serial.printf("Progress: %u%%\r", (progress / (total / 100)));
        })
        .onError([](ota_error_t error) {
          Serial.printf("Error[%u]: ", error);
          if (error == OTA_AUTH_ERROR) Serial.println("Auth Failed");
          else if (error == OTA_BEGIN_ERROR) Serial.println("Begin Failed");
          else if (error == OTA_CONNECT_ERROR) Serial.println("Connect Failed");
          else if (error == OTA_RECEIVE_ERROR) Serial.println("Receive Failed");
          else if (error == OTA_END_ERROR) Serial.println("End Failed");
      });
  
      ArduinoOTA.begin();
    }else {
      display.println("Wifi : Connected");
      display.setCursor(0,8);
      display.println("IP : " + WiFi.localIP().toString());
      display.setCursor(0,16);
      display.println("RSSI : " + String(WiFi.RSSI()) + " dBm");
    }
  
    ArduinoOTA.handle();
    
    display.setCursor(0,24);
    
    if (isnan(C)) {
      display.println("Temp : Error");
    }else {
      sprintf(str, "%4.2f", C);
      display.println("Temp : " + String(str) + " C");
      preferences.putDouble("C", C);
    }
    display.display();
    
    vTaskDelay(pdMS_TO_TICKS(100));
  }

  vTaskDelete(NULL);
}

