float CAL(float C) {
  for(int i = 0;i < CAL_RANGE;i++) {
    if (C == MAX_CAL[i]) {
      C = GEN_CAL[i] + CAL_ADJUST;
      return C;
    }
  }

  if (C >= MAX_CAL[CAL_RANGE - 1]) {
    C = GEN_CAL[CAL_RANGE - 1] + CAL_ADJUST;
    return C;
  }else if (C <= MAX_CAL[0]) {
    C = GEN_CAL[0] + CAL_ADJUST;
    return C;
  }else {
    return 0.00;
  }
}

void rgb_color(String color) {
  if (color == "red") {
    digitalWrite(RGB[0], LOW);
    digitalWrite(RGB[1], HIGH);
    digitalWrite(RGB[2], HIGH);
  }else if (color == "green") {
    digitalWrite(RGB[0], HIGH);
    digitalWrite(RGB[1], LOW);
    digitalWrite(RGB[2], HIGH);
  }else if (color == "blue") {
    digitalWrite(RGB[0], HIGH);
    digitalWrite(RGB[1], HIGH);
    digitalWrite(RGB[2], LOW);
  }else if (color == "off") {
    digitalWrite(RGB[0], HIGH);
    digitalWrite(RGB[1], HIGH);
    digitalWrite(RGB[2], HIGH);
  }
}

